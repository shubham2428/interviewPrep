@model IEnumerable<BusinessObjects.EventsBO>

@{
    ViewBag.Title = "EventsInvitedTo";
}

<h2>EventsInvitedTo</h2>


<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.NameOfTheEvent)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.ActionLink(item.NameOfTheEvent,"Details",new { id = item.EventId })
        </td>
        
    </tr>
}

</table>
